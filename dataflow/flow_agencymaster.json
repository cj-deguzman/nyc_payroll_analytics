{
	"name": "flow_agencymaster",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_AgencyMaster",
						"type": "DatasetReference"
					},
					"name": "srcagencymaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "db_NYC_Payroll_AGENCY_MD",
						"type": "DatasetReference"
					},
					"name": "snkagencymaster"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> srcagencymaster",
				"srcagencymaster sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> snkagencymaster"
			]
		}
	}
}